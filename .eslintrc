{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": "off",
    "semi": ["error", "never"],
    "react/jsx-filename-extension": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "global-require": "off",
    "import/no-named-as-default": "off",
    "jsx-a11y/label-has-for": "off"
  },
  "globals": {
    "document": true,
    "window": true,
    "__DEV__": true
  }
}
